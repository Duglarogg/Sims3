<?xml version="1.0" encoding="utf-8" ?>
<base>
  <Current_Tuning>
    <kDebugging value="True" />
    <!-- Whether to perform logging and allow debug interactions. -->
    

    <kBaseActivityChance value="20" />
    <!-- Range: 0 - 100. Base chance of alien activity on a given day. -->
    
    <kBaseAbductionChance value="25" />
    <!-- Range: 0 - 100. Base chance that alien activity is an abduction. -->
    
    <kBaseVisitChance value="50" />
    <!-- Range: 0 - 100. Base chance that alien activity is a visit. -->
    
    <kActiveAbductionChance value="50" />
    <!-- Range: 0 - 100. Base chance that active household is targeted for abduction. -->
    
    <kActiveVisitChance value="20" />
    <!-- Range: 0 - 100. Base chance that active household is targeted for visit. -->
    
    <kHourToStartActivity value="22" />
    <!-- Range: 0 - 23. Earliest hour of the day that alien activity can occur. -->
    
    <kHoursOfActivity value="6" />
    <!-- Range: 0 - 24. Span of time (in hours) during which alien acitivty can occur. -->
    
    <kSpaceRockBonus value="15" />
    <!-- Range: 0 - 100. Bonus chance of alien activity per space rock found. -->
    
    <kMaxSpaceRockBonus value="60" />
    <!-- Range: 0 - 100. Max bonus chance of alien activity for finding space rocks. -->
    
    <kTelescopeUsedBonus value="25" />
    <!-- Range: 0 - 100. Bonus chance of alien activity for using telescope. -->

    <kHighLTRThreshold value="40" />
    <!-- Range: 0 - 100. Minimum LTR liking score to qualify for the high LTR bonus. -->
    
    <kHighLTRBonus value="8" />
    <!-- Range: 0 - 100. Bonus chance that active household is target for a visit for high LTR. -->

    <kOffspringBonus value="10" />
    <!-- Range: 0 - 100. Bonus chance that active household is target for a visit for being offspring. -->

    <kSpaceRockThreshold value="5" />
    <!-- Minimum number of space rocks on lot needed to get the space rock bonus to visit chance. -->    
    
    
    <kAdvancedTechSkill value="8,10" />
    <!-- Range of Advanced Technology skill levels for NPC aliens. -->
    
    <kHandinessSkill value="5,7" />
    <!-- Range of Handiness skill levels for NPC aliens. -->
    
    <kLogicSkill value="8,10" />
    <!-- Range of Logic skill levels for NPC aliens. -->
    
    <kFutureSim value="True" />
    <!-- Whether to apply the Future Sim trait to aliens. -->
    

    <kAllowTeens value="True" />
    <!-- Whether teens can be impregnated during an abduction. -->
    
    <kImpregnationChance value="33" />
    <!-- Range: 0 - 100. Base chance of impregnation during an abduction. -->
    
    <kPregnancyLength value="3" />
    <!-- Minimum: 1. Duration (in days) of main phase for alien pregnancy. -->
    
    <kLaborLength value="4" />
    <!-- Minimum: 1. Duration (in hours) of labor phase for alien pregnancy. -->
    
    <kHoursOfPregnancy value="76" />
    <!-- Total duration (in hours) of alien pregnancy. -->
    <!-- (kPregnanyLength * 24) + kLaborLength -->
    
    <kHoursToShowPregnancyMorph value="60" />
    <!-- Span of time (in hours) to morph from not showing to fully pregnant. -->
    <!-- (5 / 6) * (kPregnancyLength * 24) -->
    
    <kHourToStartLabor value="72" />
    <!-- Hour of alien pregnancy to start labor phase. -->
    <!-- (kPregnancyLength * 24) -->
    
    <kHourToStartPregnantWalk value="32" />
    <!-- Hour of alien pregnancy to start pregnant walk. -->
    <!-- (4 / 9) * (kPregnancyLength * 24) -->
    
    <kUseFertility value="False" />
    <!-- Whether to apply fertility factors when determining the chance of impregnation during abduction. -->


    <kExamDuration value="2" />
    <!-- Duration (in hours) it takes for scientists to examine an alien baby. -->

    <kBaseCompensation value="500" />
    <!-- Base rate of compensation for volunteering for examination. -->

    <kMaxMultiplier value="6" />
    <!-- Max possilbe multiplier of compensation for volunteering for examination. -->
  </Current_Tuning>
</base>